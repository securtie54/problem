
while True:    
    s1 = int(input("1.sayı :"))
    s2 = int(input("2.sayı :"))
    ys = s1 * 2
    if ys < s2 and  s1 != s2 :
        raise Exception("1.sayının 2 katından daha küçük 2.sayı girin ve farklı sayılar girin")
    if not type(s1)  is int:
        raise Exception("int tipi değer gir a*k evladı")
    for i in range(s2,ys):
        if  (ys % i) / 2 == 1 :
            print(i)